{
  "version": 2,
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/python",
      "config": { "runtime": "python3.10" }
    },
    {
      "src": "frontend/wsgi.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.10" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/wsgi.py"
    }
  ]
}
